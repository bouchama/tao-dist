#!/usr/bin/env bash

touch tao/install/log/install.log
tail -f tao/install/log/install.log &

php tao/scripts/taoInstall.php \
--db_driver pdo_pgsql \
--db_host $DB_HOST \
--db_name $DB_NAME \
--db_user $DB_USERNAME \
--db_pass $DB_PASSWORD \
--module_namespace http://opened/tao.rdf \
--module_url $ROOT_URL \
--user_login $ADMIN_USERNAME \
--user_pass $ADMIN_PASSWORD \
-e taoCe,taoLti,taoLtiBasicOutcome,taoEventLog,taoProctoring,ltiDeliveryProvider,taoCaliper,taoQtiTest

php tao/scripts/installExtension.php taoEventLog
php tao/scripts/installExtension.php taoProctoring